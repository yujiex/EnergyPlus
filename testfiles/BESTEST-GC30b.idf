    1;                         !- Kusuda-Achenbach Phase Shift of Minimum Ground Surface Temperature {days}

Site:GroundDomain:BESTEST:GroundSurfaceConditions,
    CoupledSlab,               !- Object Name
    IngradeCoupledSlab,        !- Ground Domain Name
    ConstConvCoefficient,      !- Ground Surface Conditions
    100,                       !- Ground Surface Convection Coefficient
    10;                        !- Ground Surface Temperature

Material,
    Slab Material-In-grade,  !- Name
    MediumRough,             !- Roughness
    0.1285059,               !- Thickness {m}
    1.9,                     !- Conductivity {W/m-K}
    1490,                    !- Density {kg/m3}
    1800,                    !- Specific Heat {J/kg-K}
    0.9000000,               !- Thermal Absorptance
    0.6500000,               !- Solar Absorptance
    0.6500000;               !- Visible Absorptance

Material,
    Slab Material-Top Layer, !- Name
    MediumRough,             !- Roughness
    0.01270,                 !- Thickness {m}
    1.9,                     !- Conductivity {W/m-K}
    1490,                    !- Density {kg/m3}
    1800,                    !- Specific Heat {J/kg-K}
    0.9000000,               !- Thermal Absorptance
    0.6500000,               !- Solar Absorptance
    0.6500000;               !- Visible Absorptance

Construction,
    FLOOR-SLAB-1,            !- Name
    Slab Material-Top Layer; !- Outside Layer

SurfaceProperty:OtherSideConditionsModel,
    GroundCoupledOSCM,       !- Name
    GroundCoupledSurface;    !- Type of Modeling

  Material:NoMass,
    INSULATION-EXT,          !- Name
    VerySmooth,              !- Roughness
    50.00,                   !- Thermal Resistance {m2-K/W}
